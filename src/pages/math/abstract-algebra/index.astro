---
import NoteLayout from '../../../layouts/NoteLayout.astro';
import content from './content.html?raw';

const title = 'Abstract Algebra';
const sidebarItems = [
  { title: 'Groups', href: '#groups' },
  { title: 'Rings', href: '#rings' },
  { title: 'Fields', href: '#fields' },
